{
  "name": "iso3166-1",
  "version": "0.2.1",
  "description": "Tiny, fast, modular ISO 3166-1 alpha-2/alpha-3 parser.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/moimikey/iso3166-1.git"
  },
  "keywords": [
    "iso3166",
    "country",
    "countries",
    "iso",
    "locale"
  ],
  "author": "Michael Scott Hertzberg <mshertzberg@gmail.com> (http://michael.hertzberg.co/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moimikey/iso3166-1/issues"
  },
  "homepage": "https://github.com/moimikey/iso3166-1",
  "devDependencies": {
    "csv2": "^0.1.1",
    "event-stream": "^3.3.0",
    "file-size": "^1.0.0",
    "hyperquest": "^1.2.0",
    "jest-cli": "^0.4.0",
    "jsonstream2": "^1.1.0",
    "through2": "^0.6.5"
  },
  "optionalDependencies": {
    "browserify": "^9.0.8",
    "brfs": "^1.4.0",
    "uglifyjs": "^2.4.10",
    "zlib-browserify": "^0.0.3"
  },
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "browser": {
    "fs": false
  },
  "scripts": {
    "build": "browserify index.js > build/iso3166-1.js && cat build/iso3166-1.js | uglifyjs --screw-ie8 -c -m > build/iso3166-1.min.js",
    "generate": "bin/generate",
    "test": "jest"
  },
  "jest": {
    "rootDir": "src",
    "testPathIgnorePatterns": [
      "/__tests__/[^/]*/.+",
      "node_modules"
    ]
  }
}
