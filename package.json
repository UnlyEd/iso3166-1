{
  "name": "iso3166-1",
  "version": "0.2.5",
  "description": "Tiny, fast, modular ISO 3166-1 alpha-2/alpha-3 parser.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/moimikey/iso3166-1.git"
  },
  "keywords": [
    "iso3166",
    "iso3166-1",
    "alpha 2",
    "alpha 3",
    "iso 3166 alpha",
    "iso 3166",
    "country",
    "countries",
    "country code",
    "iso",
    "locale"
  ],
  "author": "Michael Scott Hertzberg <mshertzberg@gmail.com> (http://hertzber.gs/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moimikey/iso3166-1/issues"
  },
  "homepage": "https://github.com/moimikey/iso3166-1",
  "devDependencies": {
    "csv2": "0.1.1",
    "event-stream": "3.3.2",
    "file-size": "1.0.0",
    "harmonize": "1.4.4",
    "hyperquest": "1.3.0",
    "jest-cli": "0.10.0",
    "jsonstream2": "1.1.1",
    "through2": "2.0.1"
  },
  "optionalDependencies": {
    "brfs": "1.4.3",
    "browserify": "13.0.0",
    "uglifyjs": "2.4.10",
    "zlib-browserify": "0.0.3"
  },
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "browser": {
    "fs": false
  },
  "scripts": {
    "prepublish": "npm run generate && npm run build && npm run test",
    "build": "browserify src/index.js --full-paths --standalone iso31661 --outfile lib/index.js",
    "generate": "bin/generate",
    "test": "jest"
  },
  "jest": {
    "rootDir": "src",
    "testPathIgnorePatterns": [
      "/__tests__/[^/]*/.+",
      "node_modules"
    ]
  }
}
